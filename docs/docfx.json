{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/Primitively.csproj",
            "**/Primitively.AspNetCore.csproj",
            "**/bin/**",
            "**/obj/**",
            "**/[Tt]ests/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "globalMetadata": {
      "_appName": "Primitively",
      "_appTitle": "Primitively",
      "_appLogoPath": "images/icon.png",
      "_appFaviconPath": "images/icon.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_copyrightFooter": "Â© Electronic Hands. All rights reserved.",
      "_lang": "en",
      "pdf": false
    },
    "template": ["default", "templates/singulinkfx"],
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "dest": "_site",
    "disableGitFeatures": true,
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false
  }
}